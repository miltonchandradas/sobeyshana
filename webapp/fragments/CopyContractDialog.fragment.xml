<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout">
	<Dialog id="copyContractFragment" draggable="true" title="Copy Contract Dialog" icon="sap-icon://copy">
		<layout:VerticalLayout class="sapUiSmallMarginBegin">
			<Label text="Please enter dates for the new contract" class="sapUiSmallMarginTop sapUiMediumMarginBottom"></Label>
			<Label text="Effective Date:"></Label>
			<DatePicker class="sapUiMediumMarginBottom"></DatePicker>
			<Label text="End Date:"></Label>
			<DatePicker class="sapUiMediumMarginBottom"></DatePicker>
		</layout:VerticalLayout>
		<beginButton>
			<Button text="Copy Contract" press=".onConfirmCopyContractDialog"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press=".onCancelCopyContractDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>